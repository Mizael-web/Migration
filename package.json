{
  "name": "gestao-estoque",
  "version": "1.0.0",
  "description": "Sistema de gestão de estoque com autenticação JWT",
  "main": "index.js",
  "scripts": {
 "start": "node index.js -- watch",
  "db:migrate": "sequelize db:migrate",
 "db:migrate:undo": "sequelize db:migrate:undo",
 "db:migrate:undo:all": "sequelize db:migrate:undo:all",
 "db:seed": "sequelize db:seed:all",
 "db:seed:undo": "sequelize db:seed:undo:all",
 "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
 "db:fresh": "npm run db:migrate:undo:all && npm run db:migrate",
 "db:status": "sequelize db:migrate:status"
  },
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
